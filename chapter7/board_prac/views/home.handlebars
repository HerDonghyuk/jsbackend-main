<h1>{{title}}</h1>

<input
  type="text"
  name="search"
  id="search"
  value="{{search}}"
  size="50"
  placeholder="검색어 입력"
/>
<button onclick="location.href=`/?search=${document.getElementById("search")`}">검색</button>
<br />

<a href="/write">글쓰기</a>
<br />
{{!-- prettier-ignore --}}
<div>
  <table>
    <thead>
      <tr>
        <th width="50%">제목</th>
        <th>작성자</th>
        <th>조회수</th>
        <th>작성일</th>
      </tr>
    </thead>
    <tbody>
      {{!-- {{each posts}} --}}
        <tr>
          {{!-- <td><a href="/detail/{{_id}}">{{title}}</a></td>
          <td>{{writer}}</td>
          <td>{{hits}}</td>
          <td>{{dateString createdDt}}</td> --}}
        </tr>
      {{!-- {{/each}} --}}
    </tbody>
  </table>
</div>

<div>
  {{!-- {{#with paginator}} --}}
  <a>&lt;&lt;</a>
  <a>&lt;</a>
  <a>1</a>
  <a>2</a>
  <a>3</a>
  <a>4</a>
  <a>5</a>
  <a>&gt;</a>
  <a>&gt;&gt;</a>
  {{!-- {{/with}} --}}
</div>

